<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MonkTextView">
        <attr name="typeface" />
    </declare-styleable>

    <!-- Changing the attribute must be reflected in the TypefaceManager -->
    <attr name="typeface" format="enum">
        <enum name="opensans_regular" value="0" />
        <enum name="opensans_bold" value="1" />
        <enum name="opensans_semibold" value="2" />
        <enum name="opensans_italic" value="3" />
        <enum name="opensans_light" value="4" />
    </attr>

</resources>